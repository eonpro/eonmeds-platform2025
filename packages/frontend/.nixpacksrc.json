{
  "providers": ["node"],
  "buildPlan": {
    "phases": {
      "setup": {
        "nixPkgs": ["nodejs-20_x", "npm-10_x"]
      },
      "install": {
        "cmds": [
          "npm install --force"
        ]
      },
      "build": {
        "cmds": [
          "CI=true npm run build"
        ]
      }
    }
  },
  "start": {
    "cmd": "npx serve -s build -l $PORT"
  }
} 