{
  "name": "@eonmeds/backend",
  "version": "1.0.0",
  "private": true,
  "description": "EONMeds Backend API Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "db:migrate": "node dist/config/migrate.js",
    "db:seed": "node dist/config/seed.js"
