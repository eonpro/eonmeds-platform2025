  "keywords": [
    "api",
    "healthcare",
    "backend"
  ],
  "author": "EONMeds Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "dotenv": "^17.2.0",
    "express-jwt": "^8.5.1",
    "jwks-rsa": "^3.2.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.10"
  }
