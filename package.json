{
  "name": "@eonmeds/backend",
  "version": "1.0.0",
  "private": true,
  "description": "EONMeds Backend API Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd packages/backend && npm run build",
    "build:frontend": "cd packages/frontend && npm run build",
    "start": "node dist/index.js",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd packages/backend && npm test",
    "test:frontend": "cd packages/frontend && npm test -- --watchAll=false",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:backend": "cd packages/backend && npm run lint",
    "lint:frontend": "cd packages/frontend && npm run lint",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md,css,scss}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,jsx,json,md,css,scss}'",
    "typecheck": "npm run typecheck:backend && npm run typecheck:frontend",
    "typecheck:backend": "cd packages/backend && npm run typecheck",
    "typecheck:frontend": "cd packages/frontend && npm run typecheck",
    "quality": "npm run lint && npm run typecheck && npm run test",
    "quality:backend": "cd packages/backend && npm run quality",
    "quality:frontend": "cd packages/frontend && npm run quality",
    "install:all": "npm install && cd packages/backend && npm install && cd ../frontend && npm install",
    "db:migrate": "node dist/config/migrate.js",
    "db:seed": "node dist/config/seed.js"
  },
  "keywords": [
    "api",
    "healthcare",
    "backend"
  ],
  "author": "EONMeds Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18 <=22"
  },
  "dependencies": {
    "dotenv": "^17.2.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "prettier": "^3.2.4"
  }
}
